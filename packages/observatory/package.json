{
	"name": "observatory",
	"version": "0.9.0",
	"description": "Disk usage analyzer",
	"main": "target/main/index.js",
	"renderer": "target/renderer/index.html",
	"author": "McKayla Washburn",
	"license": "MIT",
	"repository": "https://github.com/partheseas/observatory",
	"bugs": "https://github.com/partheseas/observatory/issues",
	"config": {
		"forge": "forge.config.js"
	},
	"targets": {
		"main": {
			"context": "electron-main",
			"engines": {
				"electron": ">= 11.x.x"
			}
		},
		"renderer": {
			"context": "electron-renderer",
			"publicUrl": ".",
			"engines": {
				"electron": ">= 11.x.x"
			}
		}
	},
	"dependencies": {
		"@mckayla/electron-redux": "^2.0.1",
		"drivelist": "^9.0.0",
		"electron-squirrel-startup": "^1.0.0",
		"immer": "^8.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.54",
		"@electron-forge/maker-dmg": "^6.0.0-beta.54",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.54",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-redux": "^7.1.7",
		"electron": "11.0.3",
		"parcel": "^2.0.0-nightly.400",
		"sass": "^1.29.0"
	},
	"scripts": {
		"configure": "./scripts/hotwire.sh",
		"build": "yarn build:telescope && yarn build:main && yarn build:renderer",
		"build:main": "parcel build --target main src/main/index.ts",
		"build:renderer": "parcel build --target renderer src/renderer/index.html",
		"build:telescope": "electron-build-env neon build telescope --release",
		"dev": "yarn build && electron-forge start",
		"forge": "yarn build && electron-forge make",
		"lint": "eslint --ext .ts,.tsx .",
		"test": "jest",
		"type-check": "tsc -p . --noEmit"
	}
}
