{
	"name": "observatory",
	"version": "0.3.0",
	"description": "Disk usage analyzer",
	"main": "target/main/index.js",
	"renderer": "target/renderer/index.html",
	"author": "McKayla Washburn",
	"license": "MIT",
	"repository": "https://github.com/partheseas/observatory",
	"bugs": "https://github.com/partheseas/observatory/issues",
	"config": {
		"forge": "forge.config.js"
	},
	"targets": {
		"main": {
			"context": "electron-main",
			"engines": {
				"electron": ">= 8.x.x"
			}
		},
		"renderer": {
			"context": "electron-renderer",
			"publicUrl": ".",
			"engines": {
				"electron": ">= 8.x.x"
			}
		}
	},
	"dependencies": {
		"@mckayla/electron-redux": "^2.0.0",
		"drivelist": "^8.0.6",
		"electron-squirrel-startup": "^1.0.0",
		"gardens": "^4.2.0",
		"immer": "^6.0.3",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.50",
		"@electron-forge/maker-dmg": "^6.0.0-beta.50",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.50",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"@types/react-redux": "^7.1.7",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"electron": "^8.1.1",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.19.0",
		"parcel": "^2.0.0-nightly.183",
		"prettier": "^2.0.2",
		"sass": "^1.26.3",
		"source-map-support": "^0.5.12",
		"typescript": "^3.8.3"
	},
	"scripts": {
		"build": "yarn build:main && yarn build:renderer",
		"build:dead": "yarn build:main --no-cache && yarn build:renderer --no-cache",
		"build:main": "parcel build --target main src/main.ts",
		"build:renderer": "parcel build --target renderer src/renderer/index.html",
		"dev": "yarn build:dead && electron-forge start",
		"fmt": "prettier --ignore-path .gitignore --write .",
		"forge": "yarn build && electron-forge make",
		"lint": "eslint --ext .ts,.tsx ."
	}
}
