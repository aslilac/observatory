{
	"name": "observatory",
	"version": "2.0.0",
	"description": "Disk usage analyzer",
	"main": "./target/main/index.js",
	"renderer": "./target/renderer/index.html",
	"author": "McKayla Washburn <mckayla@hey.com>",
	"license": "MIT",
	"repository": "https://github.com/aslilac/observatory",
	"bugs": "https://github.com/aslilac/observatory/issues",
	"targets": {
		"renderer": {
			"context": "browser",
			"publicUrl": ".",
			"engines": {
				"electron": ">= 16.x.x"
			}
		}
	},
	"config": {
		"forge": "./forge.config.js"
	},
	"dependencies": {
		"@mckayla/electron-redux": "^3.0.1",
		"drivelist": "^9.0.0",
		"electron-squirrel-startup": "^1.0.0",
		"immer": "^8.0.0",
		"react": "^17.0.1",
		"react-chic": "^0.4.0",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.0",
		"redux": "^4.1.2"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.63",
		"@electron-forge/maker-dmg": "^6.0.0-beta.63",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.63",
		"@electron-forge/maker-zip": "^6.0.0-beta.63",
		"@mckayla/eslint-plugin": "^0.1.15",
		"@mckayla/prettier": "^1.0.1",
		"@mckayla/tsconfig": "^1.0.2",
		"@parcel/transformer-sass": "^2.2.1",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/react-redux": "^7.1.22",
		"concurrently": "^7.0.0",
		"electron": "17.0.0",
		"electron-rebuild": "^3.2.7",
		"eslint": "^8.8.0",
		"nodemon": "^2.0.15",
		"parcel": "^2.2.1",
		"prettier": "^2.5.1",
		"sass": "^1.49.4",
		"typescript": "^4.5.5"
	},
	"scripts": {
		"build": "npm run build:main && npm run build:renderer",
		"build:main": "tsc -p tsconfig.build.json",
		"build:renderer": "parcel build --target renderer ./src/renderer/index.html",
		"dev": "concurrently 'npm run dev:main' 'npm run dev:renderer' 'nodemon'",
		"dev:main": "tsc -p tsconfig.build.json -w",
		"dev:renderer": "parcel watch --target renderer ./src/renderer/index.html",
		"fmt": "prettier --write .",
		"forge": "npm run build && electron-forge make",
		"lint": "eslint .",
		"test": "jest",
		"type-check": "tsc -p . --noEmit"
	}
}
